--farming thing?--
--by craig--

--selection--
sel=0
px=0 py=0
things={"barn","house","tree","pond","land"}	
tiles={}
daytime=0
mapx=25 mapy=55
prevmouse=0
mousex=0 mousey=0
camx=144 camy=64
_camx=144 _camy=64

function _init()
	--init code	
	camera(camx,camy)
	cls(0)
	poke(0x5f2d, 1)
	map(0,0)	
	clouds={}
	for i=0,22 do
		add(clouds,makecloud())
	end

	for r=1,mapx do
  tiles[r] = {}
  for c=1,mapy do
  	if c%2==0 then
  		tiles[r][c]=nobuild()
  		local _x=(-2+r*16)/8
  		local _y=(3+c*4)/8  		
  		if mget(_x,_y)<76 then
   		tiles[r][c]=0
   	end
  	else
  		tiles[r][c]=nobuild()
  		local _x=(6+r*16)/8
  		local _y=(3+c*4)/8 
  		if mget(_x,_y)<76 then
   		tiles[r][c]=0
   	end
  	end
  end
	end	
	for u=1,#tiles do
		for z=1,#tiles[u] do
			if tiles[u][z]!=0 then
				if tiles[u][z].w==-99 then
					--del(tiles[u],z)
				end
			elseif rnd(1)>.92 then
				--newtree(u,z)
			end
		end
	end
end

function _update()
	daytime=t()/24
	for c in all(clouds) do
		c.update()
	end

	if (btnp(ƒ)) sel=(sel+1)%#things
	if (btnp(”)) sel=(sel-1)%#things
			
	if prevmouse==0 and stat(34==2) then
		mousex=stat(32)
		mousey=stat(33)
		_camx=camx _camy=camy
	elseif stat(34)>2 then
		camx=mid(_camx+mousex-stat(32),0,288)
		camy=mid(_camy+mousey-stat(33),-64,120)
	end
	prevmouse=stat(34)
		
	px=flr((stat(32)+camx)/16)
	py=flr((stat(33)+camy)/4)
	px=mid(px,1,#tiles)
	py=mid(py,1,#tiles[px])
	
	if stat(34)==1 and
		tiles[px][py]==0 then
		if (sel==0) newbarn(px,py)
		if (sel==1) newhouse(px,py)
		if (sel==2) newtree(px,py)
		if (sel==3) newpond(px,py)
		if (sel==4) newland(px,py)
	end
			
	for x=1,#tiles do
		for y=1,#tiles[x] do
			if tiles[x][y]!=0
				and tiles[x][y]!=nil then
					tiles[x][y].update(tiles[x][y])
			end
		end
	end
end

function _draw()
	cls(palget(12,.5-sin(daytime)/2))
	camera(camx,camy)
	palt(3,false)
	palt(0,true)
	--sun()
	for c in all(clouds) do
		c.draw()
	end
	palt(0,false)
	map(0,0)
	if (btn(Ž)) grid()
	pal(0,0)
	palt(0,false)
	palt(3,true)
	for x=1,#tiles do
		for y=1,#tiles[x] do
			if tiles[x][y]!=0
				and tiles[x][y]!=nil then
					tiles[x][y].draw(tiles[x][y])
			end
		end
	end
	
	x=1+(px*16)
	if (py%2==0) x-=7
	pal()
	palt(3,true)
	if (tiles[mid(px,1,mapx)][mid(py,1,mapy)]!=0) pal(11,8)
	if (sel==0) sspr(32,0,14,11,x,-4+py*4)
	if (sel==1) sspr(48,0,14,11,x,-4+py*4)
	if (sel==2) sspr(8,8,8,8,x+4,-4+py*4)
	if (py%2==0) x-=1
	if (sel==3) sspr(0,24,15,7,x,py*4)
	if (sel==4) sspr(0,24,15,7,x,py*4)
	pal()
	palt(3,true)
	--cursor	
	sspr(0,0,6,7,camx+stat(32),camy+stat(33))
	
	rain()
end

function drawmenu()
	rectfill(1+camx,1+camy,34+camx,6+#things*10+camy,0)
	local x=10
	local y=camy
	for i=1,#things do
		if (i==sel+1) x=14+camx
		if (i!=sel+1) x=10+camx
		print(things[i],x,-3+y+i*10,13)
		print(things[i],x,-4+y+i*10,7)
	end
	--menu	pointer
	sspr(0,32,11,10,2+camx,4+camy+10*sel)	
end

function grid()
	fillp(0b0101010101010101.1)
	for x=1,mapx do
		line(x*8,127+x*4,207+x*8,24+x*4,11)
		line(x*8,127-x*4,207+x*8,230-x*4,11)
	end
	fillp()
end
