--tiles--

function getpxlposx(tx,ty,ox)	
	local x=-1+(tx*16)
	if (ty%2==0) x-=7-ox
	cls(0)
	return x+2
end
	
function nobuild()
	local no={w=nil,
		update=function(n)
		end,
		draw=function(n)
		end
	}
	return no
end

function newbarn(_tx,_ty)
	local b={
		x=getpxlposx(_tx,_ty,0),y=-4+_ty*4,
		tx=_tx,ty=_ty,w=0,
		b=0,
		update=function(h)
			if (time()%.5==0) h.b=mid(h.b+1,0,6)
		end,
		draw=function(h)
			sspr(h.b*16,80,14,11,h.x,h.y)
		end
	}
	tiles[_tx][_ty]=b
end

function newhouse(_tx,_ty)
	local h= {
		x=getpxlposx(_tx,_ty,0),
		y=-4+_ty*4,tx=_tx,ty=_ty,w=0,
		b=0,
		smoke={},
		update=function(h)
			if (time()%.5==0) h.b=mid(h.b+1,0,7)
		end,
		
		draw=function(h)
			sspr(h.b*16,64,14,11,h.x,h.y)
			if h.b>6 then
				for i=0,2 do
					local r=rnd(5)
					circfill(h.x+6-r,h.y-r,r/3,0)
				end
			end
		end
	}
	tiles[_tx][_ty]=h
end

function newtree(_tx,_ty)
	local t={	
		kind=flr(rnd(4)),
		x=getpxlposx(_tx,_ty,0)+4,
		y=-4+_ty*4,tx=_tx,ty=_ty,w=0,
		update=function(t)
		end,
		draw=function(t)
			palt(0,true)
			palt(3,false)
 		sspr(120,8*t.kind,8,8,t.x,t.y)	
			palt(0,false)
			palt(3,true)
	end
	}	
	tiles[_tx][_ty]=t
end

function tl(p,x,y)
	if p.tx+x<1
		or p.ty+y<1
		or p.tx+x>#tiles
		or p.ty+y>#tiles[p.tx+x] then
		return 0
	elseif tiles[p.tx+x][p.ty+y]!=0 then
		if tiles[p.tx+x][p.ty+y].w!=nil then
			return tiles[p.tx+x][p.ty+y].w
		end
	else
		return 0
	end
end

function newpond(_tx,_ty)
	local p={
		x=getpxlposx(_tx,_ty,-1),
		y=_ty*4,tx=_tx,ty=_ty,w=1,
		seem_l=false,seem_r=false,seem_m=false,
	
		update=function(p)
		end,
	
		draw=function(p)
			sspr(88,40+(8*flr(time()*5%3)),15,8,p.x,p.y)
			if (p.seem_l)	sspr(64,1,7,4,p.x,p.y)
			if (p.seem_r)	sspr(72,3,7,4,p.x+8,p.y)
			if (p.seem_m) sspr(65,3,1,2,p.x+7,p.y-1)
		end,
		
		seem=function(p)
			if p.ty%2==0 then
				if tl(p,-1,-1)>0 then
			 		p.seem_l=true
				end
				if tl(p,0,-1)>0 then
			 	p.seem_r=true
			 end			 
				if tl(p,-1,-1)>0 and
					tl(p,0,-1)>0 and
					tl(p,0,-2)>0 then
						p.seem_m=true
				end						
			else
			 if tl(p,0,-1)>0 then
	 			p.seem_l=true
  	 end
			 if tl(p,1,-1)>0 then
			 	p.seem_r=true
				end
				if tl(p,1,-1)>0 and
				 tl(p,0,-1)>0 then
					p.seem_m=true
				end			
			end
		end
		}
	p.seem(p)
	tiles[_tx][_ty]=p
	for _t in all(tiles) do
		for t in all(_t) do
			if t!=0 and t.w!=nil and t.w>0 then 
			 t.seem(t)
  	end
  end
 end
end

function newland(_tx,_ty)
	local p={
	x=getpxlposx(_tx,_ty,-1),
	y=_ty*4,tx=_tx,ty=_ty,w=0,
	update=function(p)
	end,
	
	draw=function(p)
		sspr(16,24,15,8,p.x,p.y)
	end
	}
	tiles[_tx][_ty]=p
end
