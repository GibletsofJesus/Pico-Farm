--npc--

npc={
x=88,y=97,_t=0,ni=1,
path={},
cn={x=start.x,y=start.y},--current node
tn={x=goal.x,y=goal.y},--target node

	update=function(n)
		if n.tn!=nil and n.path!=nil then
 		--move towards next node
 		local x1=n.cn.x*16
 		local x2=n.tn.x*16
 		if (n.cn.y%2==1) x1+=8
 		if (n.tn.y%2==1) x2+=8
 		
 		n.x=x1+(n._t)*(x2-x1)
			n.y=n.cn.y+(n._t)*(n.tn.y-n.cn.y)
			n._t+=0.1
			if n._t>1 then
				n.cn=n.tn
				n.ni+=1
				n.tn=n.path[#n.path-n.ni]
				n._t-=1				
			end		
		else
		end
	end,
	
	draw=function(n)
		sspr(24,0,1,3,camx+n.x+8,1+camy+n.y*4)
	end
}
