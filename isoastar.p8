--world gen--
world={}

function newgen(mapx,mapy)
	local ox=camx+1
	local oy=camy	
	for x=1,mapx do
		world[x]={}
		for y=1,mapy do
			world[x][y]=0		
			if y%2==0 then 
				if x-1>=abs((mapx-y-1)/2) and
					x-1<abs(mapx-(abs(mapx-y-1))/2) then
						world[x][y]=1
 			end
			end
			if y%2==1 and y*2>mapy/1.15 then
				if x>=abs((mapx-y-2)/2) and
					x<abs(mapx-(abs(mapx-y)/2)) then
						world[x][y]=1
					end
			elseif y%2==1 then
				if x-1>=abs((mapx-y-2)/2) and
					x-1<abs(mapx-(abs(mapx-y)/2)) then
						world[x][y]=1
				end
			end
		--wateredge(x,y,mapx,mapy)
		end
	end
end

function wateredge(x,y,mapx,mapy)
	if y%2==0 then 
		if x-2>=abs((mapx-y-1)/2) and
			x<abs(mapx-(abs(mapx-y-1))/2) then
			world[x][y]=3
 	end
	end
	if y%2==1 and y*2>mapy/1.15 then
		if x-1>=abs((mapx-y-2)/2) and
			x+1<abs(mapx-(abs(mapx-y)/2)) then
			world[x][y]=4
		end
	elseif y%2==1 then
		if x-2>=abs((mapx-y-2)/2) and
					x<abs(mapx-(abs(mapx-y)/2)) then
			world[x][y]=5
  end
	end
end
