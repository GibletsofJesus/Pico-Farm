camx=0 camy=0

function _init()
 poke(0x5f2d, 1)
 
	newgen(25,55)
	
	for x=1,#world do
		for y=1,#world[x] do
			--centre
			fillcent(x,y,#world/2,8+#world[x]/2,4)	
			fillcent(x,y,#world/2,-16+#world[x]/2,4)
			fillcent(x,y,#world/2,-4+#world[x]/2,10)					
			
			if rnd(1)>.9 and world[x][y]>0 then
				world[x][y]=1
			end			
		end
	end	
end

function fillcent(x,y,cx,cy,_d)
	local dist=(
			(cx-x)^2 +
			(cy-y)^2	)^0.5
			if dist<_d then
				world[x][y]=2				
				if dist<_d-2 and rnd(1)>.85 then
					world[x][y]=4
				end
			end
end

function _update()
	if (btn(”)) camy+=8
	if (btn(ƒ)) camy-=8
	if (btn(‹)) camx+=16
	if (btn(‘)) camx-=16
		
	px=flr((stat(32)-(camx))/16)
	py=flr((stat(33)-camy)/4)	
	px=mid(px,1,#world)
	py=mid(py,1,#world[px])
	
	npc.update(npc)
	
	if stat(34)==1 and prev!=1 then		
		if npc.tn!=nil and npc._t%1>.5 then
			start=npc.tn
		else
			start.x=npc.cn.x
			start.y=npc.cn.y
		end
		goal.x=px
		goal.y=py
		getpath()
	end
	prev=stat(34)
end
prev=0
once=false
